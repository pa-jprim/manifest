<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <!-- Build and Core -->
  <remove-project name="android_build" />
  <project path="build/make" name="android_build" remote="jprim" >
    <linkfile src="CleanSpec.mk" dest="build/CleanSpec.mk"/>
    <linkfile src="buildspec.mk.default" dest="build/buildspec.mk.default"/>
    <linkfile src="core" dest="build/core"/>
    <linkfile src="envsetup.sh" dest="build/envsetup.sh"/>
    <linkfile src="target" dest="build/target"/>
    <linkfile src="tools" dest="build/tools"/>
  </project>

  <!-- Device -->
  <remove-project name="android_device_qcom_common" />
  <project path="device/qcom/common" name="android_device_qcom_common" remote="jprim" >
    <linkfile dest="vendor/qcom/build/tasks/generate_extra_images.mk" src="generate_extra_images.mk"/>
  </project>

  <!-- Frameworks -->
  <remove-project name="android_frameworks_av" />
  <project path="frameworks/av" name="android_frameworks_av" remote="jprim" />

  <!-- Manifest -->
  <remove-project name="manifest" />
  <project path="manifest" name="manifest" remote="jprim" />

  <!-- Packages -->
  <remove-project name="platform/packages/services/DeviceAsWebcam" />
  <project path="packages/services/DeviceAsWebcam" name="android_packages_services_DeviceAsWebcam" remote="jprim" />

  <!-- Signatures -->
  <remove-project name="pa-faiz/Signing_keys" />
  <project path="vendor/aospa/signing/keys" name="Jprimero15/priv_sign_keys" remote="github-ssh" revision="master" />

  <!-- System -->
  <remove-project name="platform/system/memory/lmkd" />
  <project path="system/memory/lmkd" name="android_system_memory_lmkd" remote="jprim" />
  <remove-project name="android_system_sepolicy" />
  <project path="system/sepolicy" name="android_system_sepolicy" remote="jprim" />

  <!-- Vendor -->
  <remove-project name="android_vendor_aospa" />
  <project path="vendor/aospa" name="android_vendor_aospa" remote="jprim" >
    <linkfile src="build.sh" dest="rom-build.sh" />
  </project>

</manifest>
